# Make sure setup.py is placed under
# mosesdecoder/python
# otherwise you will have to edit:
#   include_dirs
#   libdir
#   sources
#
# from mosesdecoder/python
#
# compile using cython 
echo "##################COMPILING##############"
cython --cplus binpt/binpt.pyx
#
# then build the extension
echo "##################BUILDING##############"
python setup.py build_ext -i

